<?xml version="1.0" encoding="UTF-8"?>
<inkscape-extension xmlns="http://www.inkscape.org/namespace/inkscape/extension">
	<_name>Stitch Fabricator</_name>
	<id>com.njhurst.filter.dformerS</id>
	<dependency type="executable" location="extensions">dformerS.py</dependency>
	<dependency type="executable" location="extensions">inkex.py</dependency>
	<param name="tab" type="notebook">
        <page name="stitch" _gui-text="Stitch Method">
            <_param name="argue" type="description" xml:space="preserve">Variables:</_param>
			<param name="paraStitch" type="float" min="1e-8" max="1e10" _gui-text="Diameter:">10</param>
            <param name="pointsS" type="int" min="1" max="100" _gui-text="Number of Connectors:">20</param>
			<param name="offsetS" type="float" min="1e-8" max="1e10" _gui-text="Offset:">20</param>
		</page>
	</param>
	<effect>
		<object-type>path</object-type>
		<effects-menu>
			<submenu _name="D-Formers"/>
		</effects-menu>
	</effect>
	<script>
		<command reldir="extensions" interpreter="python">dformerS.py</command>
	</script>
</inkscape-extension>
